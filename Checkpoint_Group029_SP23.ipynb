   "source": [
    "# Merge 2023 arbitration pitchers with 2022 season statistics \n",
    "pitch_2023 = pd.merge(pitch_2023, fg_pitch, on=['Name', 'Prev. Season'], how='left').drop(['index'], axis = 1)\n",
    "\n",
    "# Change IP figure to 1/3 for .1, 2.3 for .2\n",
    "pitch_2023['IP'] = pitch_2023['IP'].apply(change_IP)\n",
    "\n",
    "# Apply the remove_percentages function to all percentage columns\n",
    "for i in ['K-BB%','CSW%','SwStr%','HardHit%','HR/FB','O-Swing%','Z-Swing%','Swing%','O-Contact%','Z-Contact%','Contact%','Zone%']:\n",
    "  pitch_2023[i] = pitch_2023[i].apply(remove_percentages)\n",
    "\n",
    "# Fill-in all pitchers who missed a year during arbitration due to injury/demotion\n",
    "pitch_2023.fillna(dict.fromkeys(pitch_2023.columns[5:], 0), inplace=True)\n",
    "\n",
    "pitch_2023"
   ]
