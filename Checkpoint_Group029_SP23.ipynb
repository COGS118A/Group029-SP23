    "from scipy.stats import zscore\n",
    "\n",
    "def convert_to_zscores(df):\n",
    "    \"\"\"\n",
    "    Convert values in numerical columns of a DataFrame to their respective z-scores.\n",
    "\n",
    "    Args:\n",
    "        df (pandas.DataFrame): The DataFrame to be converted.\n",
    "\n",
    "    Returns:\n",
    "        pandas.DataFrame: The DataFrame with values converted to z-scores.\n",
    "    \"\"\"\n",
    "    # Select only numerical columns\n",
    "    numerical_columns = df.select_dtypes(include='number').columns\n",
    "    \n",
    "    # Apply z-score transformation to numerical columns\n",
    "    df[numerical_columns] = df[numerical_columns].apply(zscore)\n",
    "    \n",
    "    return df"
   ]
