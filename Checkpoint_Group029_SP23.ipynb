    "# Apply the remove_percentages function to all percentage (string) columns\n",
    "for i in ['K-BB%','CSW%','SwStr%','HardHit%','HR/FB','O-Swing%','Z-Swing%','Swing%','O-Contact%','Z-Contact%','Contact%','Zone%']:\n",
    "  df_pitchers[i] = df_pitchers[i].apply(remove_percentages)\n",
    "\n",
    "# Fill-in all pitchers who missed a year during arbitration due to injury/demotion\n",
    "df_pitchers.fillna(dict.fromkeys(df_pitchers.columns[5:], 0), inplace=True)\n",
    "\n",
    "df_pitchers"
   ]
