    "def one_hot_encode(df, column_name):\n",
    "    \"\"\"\n",
    "    Convert a column of categorical data in a DataFrame to one-hot encoded data.\n",
    "\n",
    "    Args:\n",
    "        df (pandas.DataFrame): The DataFrame containing the column to be encoded.\n",
    "        column_name (str): The name of the column to be encoded.\n",
    "\n",
    "    Returns:\n",
    "        pandas.DataFrame: The DataFrame with the one-hot encoded column.\n",
    "    \"\"\"\n",
    "    # Extract the column to be encoded\n",
    "    column = df[column_name]\n",
    "    \n",
    "    # Perform one-hot encoding using pandas.get_dummies()\n",
    "    one_hot_encoded = pd.get_dummies(column, prefix=column_name).astype(int)\n",
    "    \n",
    "    # Concatenate the one-hot encoded column with the original DataFrame\n",
    "    df_encoded = pd.concat([df, one_hot_encoded], axis=1)\n",
    "    \n",
    "    # Drop the original column from the DataFrame\n",
    "    df_encoded.drop(column_name, axis=1, inplace=True)\n",
    "    \n",
    "    return df_encoded\n"
   ]
